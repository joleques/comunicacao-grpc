{
  "name": "service-node-client",
  "version": "1.0.0",
  "description": "Servi√ßo em node",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run build-ts && npm run lint",
    "serve": "node dist/index.js",
    "watch-node": "nodemon dist/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "eslint \"**/*.{js,ts}\" --quiet",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/index.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joleques/comunicacao-grpc.git"
  },
  "author": "jorge oleques",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joleques/comunicacao-grpc/issues"
  },
  "homepage": "https://github.com/joleques/comunicacao-grpc#readme",
  "devDependencies": {
    "@types/concurrently": "^5.2.1",
    "@types/eslint": "^7.2.6",
    "@types/eslint-config-prettier": "^6.11.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "inversify": "^5.0.5",
    "nodemon": "^2.0.12",
    "protobufjs": "^6.11.2"
  }
}
